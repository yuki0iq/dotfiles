[alias]
    git = !git
    l = log --graph --abbrev=0 --decorate --format=format:\"%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(dim white) - %an%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n %C(white)%s%C(reset)\"
    ll = !git l --all
    lll = !git ll $(git rev-list -g --all)
    cl = clone
    p = push
    pp = push --force-with-lease
    ppp = push --force
    t = pull
    f = fetch --all
    b = branch
    br = branch -d
    brr = branch -D
    bm = branch -M
    s = status
    a = add
    aa = add .
    aaa = add ..
    ai = add -i
    ap = add -p
    ua = reset HEAD
    d = diff
    dd = diff --staged
    done = diff --stat
    c = commit
    cc = commit -m
    ca = commit --amend
    co = checkout
    cob = checkout -b
    cod = checkout --detach
    rbb = reset --soft HEAD^
    me = merge
    mea = merge --abort
    mec = merge --continue
    mb = merge-base
    su = submodule
    sua = submodule add
    sus = submodule status --recursive
    sui = submodule init
    suu = submodule update
    re = rebase
    rei = rebase -i
    reo = rebase --onto
    rea = rebase --abort
    rec = rebase --continue
    res = rebase --skip
    reto = rebase --edit-todo
    bla = blame
    ff = merge --ff-only
[core]
    editor = vim
    pager = delta
[init]
	defaultBranch = main
[user]
	name = Yuki
	email = yuki.utk8g@gmail.com
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true    # use n and N to move between diff sections
    light = false      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
